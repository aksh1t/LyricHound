<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.java.j2seproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/j2se-project/3">
            <name>LyricHound</name>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <web-service-clients xmlns="http://www.netbeans.org/ns/j2se-project/jax-rpc">
            <web-service-client>
                <web-service-client-name>server.php</web-service-client-name>
                <web-service-stub-type>jaxrpc_static_client</web-service-stub-type>
                <client-source-url>http://lyrics.wikia.com/server.php?wsdl</client-source-url>
            </web-service-client>
        </web-service-clients>
    </configuration>
</project>
